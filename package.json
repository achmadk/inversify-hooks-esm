{
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/inversify-hooks-esm.esm.js",
      "require": "./dist/index.js"
    }
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "prepublishOnly": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {
    "@abraham/reflection": "0.8.0",
    "inversify-esm": "1.0.0",
    "inversify-props-esm": "1.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint --fix"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "endOfLine": "auto"
  },
  "name": "inversify-hooks-esm",
  "author": "Achmad Kurnianto",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/achmadk/inversify-props-esm.git"
  },
  "module": "dist/inversify-hooks-esm.esm.js",
  "size-limit": [
    {
      "path": "dist/inversify-hooks-esm.cjs.production.min.js",
      "limit": "512 B"
    },
    {
      "path": "dist/inversify-hooks-esm.esm.js",
      "limit": "512 B"
    }
  ],
  "devDependencies": {
    "@abraham/reflection": "0.8.0",
    "@size-limit/preset-small-lib": "^5.0.3",
    "@skypack/package-check": "0.2.2",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "babel-eslint": "10.1.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.9.1",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.1",
    "eslint-plugin-react": "7.25.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "^5.0.9",
    "inversify-esm": "1.0.0",
    "inversify-props-esm": "1.0.0",
    "prettier": "2.3.2",
    "size-limit": "^5.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2"
  },
  "keywords": [
    "inversify",
    "typescript",
    "typescript inject",
    "typescript di",
    "react di",
    "react inject",
    "react hooks",
    "es module support"
  ],
  "packageManager": "yarn@3.0.1"
}
